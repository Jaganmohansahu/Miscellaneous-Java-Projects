<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>x</servlet-name>
		<jsp-file>/s9.jsp</jsp-file>
		
		<init-param>
			<param-name>course</param-name>
			<param-value>JAVA</param-value>
		</init-param>
		<init-param>
			<param-name>fees</param-name>
